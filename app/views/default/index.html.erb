<div class="navigation">
  <%= render :partial=>'sidenav' %>
</div>

<div class="content">

  <% if user_signed_in? %>
    Welcome <%=current_user.email%> | <%= link_to 'Logout', {:controller=>:users, :action=>:sign_out}, :method=>:delete %>
  <% else %>
    <%= link_to 'Login', {:controller=>:users, :action=>:sign_in} %> | <%= link_to 'Sign up', {:controller=>:users, :action=>:sign_up} %>
  <% end %>
  
  <div id="navTabs">
    
    <ul>
        <li><a href="#taxa">Taxa and Ecosystems</a></li>
        <li><a href="#interactions">Interaction Types</a></li>
        <li><a href="#observations">Observations</a></li>
        <li><a href="#viz">Visualize</a></li>
     </ul>
     
     <div id="taxa">
       
       <%= render :partial=>'taxa' %>

     </div>

     <div id="interactions">
       
       <%= render :partial=>'interactions' %>

     </div>

     <div id="observations">
       
       <%= render :partial=>'observations' %>

     </div>
     
     <div id="viz">
     
      <%= render :partial=>'visualize' %>
     
     </div>
       
  </div>

  <script>
  	$(function() {
  		$( "#navTabs" ).tabs();
  	});
  </script>

  <br />
    
 
</div>
